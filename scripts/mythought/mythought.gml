	dialogue[25,4] = "Bahkan ada yang membuat semestanya menjadi satu miliar tahun dalam satu hari"
	dialogue[25,5] = "Untuk dunia ini dan semesta 0-0-0 memiliki [Dilatasi Waktu] 1:1"
	
	// 26: topic: Universe 0-0-0
	dialogue[26,0] = "Aku sering bilang jika diriku adalah makhluk semesta 0-0-0. Tetapi apa itu universe 0-0-0?"
	dialogue[26,1] = "Garis waktu 0, Ranting Waktu 0, dan Simpangan Waktu 0"
	dialogue[26,2] = "Dan juga Realita Linear 0, Semesta 0, Multi Semesta 0"
	dialogue[26,3] = "Jadi intinya, garis waktu dan semesta kami seharusnya dibilang sebagai 'Realita Sempurna'"
	dialogue[26,4] = "Tetapi ternyata masalahnya lebih dari itu. Namanya juga berada di titik 0,0. Berarti semuanya akan berpusat pada semesta kami"
	dialogue[26,5] = "Maka yang harusnya ada nihil masalah, malah menjadi infinit masalah"
	dialogue[26,6] = "Itulah pula mengapa semua [Pengawas Hukum Semesta] berpusat di semesta 0-0-0"
	
	// 27: topic: Styx Rai
	dialogue[27,0] = "Kau antara tidak tahu atau belum tahu ini. Tetapi Styx Rai memiliki banyak tubuh."
	dialogue[27,1] = "Mungkin Styx adalah orang yang bisa kau bilang sebagai [Makhluk Infinit]? Karena sepertinya dia ada di setiap semesta yang aku temui. Berkebalikan dengan [Makhluk Omni]"
