	// 19: topic: least favorite Ruler
	dialogue[19,0] = "Persalius... orang itu..."
	dialogue[19,1] = "Aku sebenarnya sangat mengenal [Penguasa Aturan Makhluk Hidup] lainnya. Beberapa faktor ya karena aku memiliki kewajiban sebagai seorang [Penguasa] pula"
	dialogue[19,2] = "Dalam beberapa kejadian, ada pula beberapa kesempatan aku terpaksa bekerja dengan [Penguasa] lainnya. Untuk [Penguasa] yang aku kenal dekat seperti Aliyu dan Pandora, aku tidak mempunyai masalah dengan mereka"
	dialogue[19,3] = "Tetapi dengan Persalius... Dia terlalu mengestimasi kemampuannya. Aku mungkin akan terus bersikap seperti seseorang yang mengoceh dari belakang jika aku terus begini"
	dialogue[19,4] = "Aku hanya tidak bisa berada didekatnya. Begitu saja"
	dialogue[19,5] = "Dan orang macam apa yang tidak menyukai coklat"
	
	// 20: topic: Kepingan Halo Kreationis
	dialogue[20,0] = "Kau sudah mengetahui apabila Saint Pandora juga adalah Pleiades Rai. Tetapi kau tidak tahu betapa kuatnya dia. Dia adalah satu-satunya orang yang aku takuti di seluruh semesta"
	dialogue[20,1] = "Seperti yang kau lihat dibelakangku, ada bulatan yang biasa kami sebut dengan 'Halo'. Setiap batang menampilkan berapa banyak semesta yang sudah seorang [Kreationis] buat."
	dialogue[20,2] = "Umumnya, seorang [Kreationis] akan kesusahan untuk membuat satu dunia. Kemudian dengan mudah dapat membuat 2 sampai 5 dunia lagi. Tetapi setelah itu, kesusahannya akan meningkat berkali-kali lipat."
	dialogue[20,3] = "Beberapa [Kreationis] yang memiliki mental kuat bisa memiliki 20 kepingan. Dan aku sendiri, aslinya memiliki 36 kepingan terima kasih kepada [Sang A]"
	dialogue[20,4] = "Tetapi aku memotong 30 batang karena aku tidak merasa pantas jika [Sang A] lah yang membuat dunia-dunia itu dengan menggunakan diriku. Dan akhirnya aku tidak pernah membuat dunia lagi"
	dialogue[20,5] = "Sampai akhirnya aku menemui Pandora. Pada pandangan pertama kau pasti akan bingung karena kau merasakan hal seperti 'orang ini kuat sekali' padahal Halo yang dia pakai tidak memiliki kepingan sama sekali"
	dialogue[20,6] = "Tetapi aku keliru. Sangatlah keliru."
	dialogue[20,7] = "Pandora bukannya tidak memiliki satupun kepingan. Tetapi kebalikannya"
	dialogue[20,8] = "Kepingan milik Pandora sangatlah berdempet sehingga kelihatannya seperti Halo kedua yang dia pakai diatas Halo utamanya"
	dialogue[20,9] = "Dengan kata lain, dia secara teori seharusnya sudah menciptakan semesta sampai tidak dapat didefinisikan lagi betapa banyaknya"
	// 21: topic: timeline?
	dialogue[21,0] = "Aku tidak tahu bagaimana cara kerjanya. Tetapi sepertinya garis waktu dunia ini selalu berganti setiap kali kau keluar dari dunia ini"
	dialogue[21,1] = "Sebagai contohnya. Mari kita lihat, garis waktu sekarang adalah: " + string(random_get_seed())
	
	
	// 22: topic: Dimension
	dialogue[22,0] = "Aku sering bilang jika dirimu adalah makhluk [Dimensi 13]. Tetapi aku tidak tahu apakah konsep dimensi di semestaku sama dengan dirimu atau tidak"
	dialogue[22,1] = "Setidaknya di semestaku dimensi sebenarnya hanya ada 12. Masing-masing 4 untuk Ruang dan Waktu, 4 Realita, dan 2 Infinit"
	dialogue[22,2] = "Maukah aku menjelaskannya lagi? Baiklah"
