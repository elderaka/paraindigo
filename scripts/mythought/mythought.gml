	dialogue[22,4] = "[Dimensi 4 - Tali Waktu] \n[Dimensi 5 - Ranting Waktu] \n[Dimensi 6 - Simpangan Waktu]"
	dialogue[22,5] = "[Dimensi 7 - Realita Linear] \n[Dimensi 8 - Semesta] \n[Dimensi 9 - Multi Semesta]"
	dialogue[22,6] = "[Dimensi 10 - Omni Semesta] \n[Dimensi 11 - Infinit Rendah] \n[Dimensi 12 - Infinit Tinggi]"
	dialogue[22,7] = "Mungkin begitulah. Apa saja isi dari mereka? Tak tahu. Tanyakan atau cari tau saja kepada para [Hakim] atau kepada Author yang berada di duniamu"
