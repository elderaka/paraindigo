	// 23: topic: Erudera's factor and Protagonist tendencies
	dialogue[23,0] = "Isagel adalah seorang Indigo, sekaligus pemegang [Faktor Erudera]"
	dialogue[23,1] = "itu berarti entah bagaimana itu, semesta atau setidaknya dunia ini akan terus berporos kepada dirinya"
