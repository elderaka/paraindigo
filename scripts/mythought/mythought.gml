	// 16: topic: Favorit Lawyer
	dialogue[16,0] = "Siapa [Hakim] favoritku? Kau pasti mengira bahwa orang itu adalah Pleiades atau Pandora. Tetapi kau salah"
	dialogue[16,1] = "Orang yang paling aku suka di kelompok [Hakim] adalah Eros Rai"
	dialogue[16,2] = "Sebagai [Pengawas Hukum Ruang dan Waktu], aku tidak menyangka bahwa dedikasinya sebesar itu"
	dialogue[16,3] = "Satu detik bagi dia bisa saja terasa seperti berjam-jam jika dia serius"
	dialogue[16,4] = "Dan umur dia kira-kira setara dengan 6 umur semesta dan bahkan aku yakin lebih dari itu"
