	dialogue[17,8] = "Aliyu juga merupakan anggota [Para Indigo], yang berarti dia juga merupakan teman dekat Alice Tetrahedrolion"
	
	// 18: topic: least favorite Lawyer
	dialogue[18,0] = "Aku sangat membenci Betelgeuse Rai. Setidaknya Betelgeuse yang lalu"
